  "source": "// SPDX-License-Identifier: MIT\n\npragma solidity ^0.8;\n\nimport \"./Auth.sol\";\n\nabstract contract Finalizable is Auth {\n    bool public isFinalized;\n\n    modifier unfinalized() {\n        require(!isFinalized, \"FINALIZED\"); _;\n    }\n\n    modifier finalized() {\n        require(isFinalized, \"!FINALIZED\"); _;\n    }\n\n    function finalize() public authorized unfinalized {\n        isFinalized = true;\n        emit Finalized();\n    }\n\n    event Finalized();\n}\n",
